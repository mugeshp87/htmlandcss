import HocComponentIncrementCount from './HocComponentIncrementCount'

export class ButtonComponent extends Component {
constructor(props) {
super(props)

this.state = {

}
}

render() {
return (
<div>
<h1>
ButtonComponent :{this.props.passcount}
</h1>
<button onClick={this.props.incrementedCount}>Increment Count</button>
</div>
)
}
}

