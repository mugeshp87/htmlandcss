mport React, { Component } from 'react'
import HocComponentIncrementCount from './HocComponentIncrementCount';

export class LabelComponent extends Component {


render() {
return (
<div><h1>Label Component:{this.props.passcount} </h1>
<label onMouseEnter={this.props.incrementedCount}>move the cursor here</label></div>
)
}
}

export default HocComponentIncrementCount(LabelComponent);

